{
 "author": {
  "name": "ruhrev"
 },
 "manifest_version": 2,
 "name": "KICK LIVE",
 "version": "1.0",
 "description": "Check the online/offline status of a streamer on Kick.com",
 "icons": {
  "16": "icons/icon16.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
 },
 "browser_action": {
  "default_popup": "popup/popup.html"
 },
 "background": {
  "scripts": [
   "scripts/background.js"
  ],
  "persistent": true
 },
 "permissions": [
  "https://kick.com/",
  "contextMenus",
  "storage"
 ]
}